{
  "name": "ask_on_youtube",
  "version": "1.0.0",
  "description": "Ask on YouTube",
  "main": "background.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "eslint src/ --fix",
    "build": "npm run lint && npm run clean && webpack",
    "build-release": "npm run lint && npm run clean && NODE_ENV=production webpack && npm run zip",
    "watch": "nodemon --watch src/ --watch webpack.config.cjs --watch manifest.json --watch _locales --ext js,html,css,json --exec 'npm run build'",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "zip": "tar -a -c -f ask_on_youtube.zip dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazuki-sf/youtube_summary.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/kazuki-sf/youtube_summary/issues"
  },
  "homepage": "https://github.com/kazuki-sf/youtube_summary#readme",
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^9.18.0",
    "eslint-plugin-jest": "^28.11.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "globals": "^15.14.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.4.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.9",
    "puppeteer": "^24.1.1",
    "rimraf": "^3.0.2",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.11",
    "util": "^0.12.5",
    "webpack": "^5.59.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "buffer": "^6.0.3",
    "jquery": "^3.7.1",
    "xmldom": "^0.6.0"
  }
}
